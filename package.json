{
	"name": "arm",
	"displayName": "ARM",
	"description": "ARM support for Visual Studio Code",
	"version": "0.3.0",
	"publisher": "dan-c-underwood",
	"icon": "images/icon.png",
	"author": {
		"name": "Dan Underwood"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dan-c-underwood/vscode-arm.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "arm",
				"aliases": [
					"ARM",
					"arm"
				],
				"extensions": [
					".s",
					".S",
					".asm",
					".sx"
				],
				"configuration": "./arm.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "arm",
				"scopeName": "source.arm",
				"path": "./syntaxes/arm.json"
			}
		]
	}
}
