        "1": {
          "patterns": [
            {
              "include": "#conditions",
              "name": "comment.nop.arm"
            },
            {
              "match": "\\w+",
              "name": "invalid.illegal.condition.arm"
            }
          ]
        }
      }
    },
    {
      "name": "storage.memaddress.arm",
      "match": "\\[[ \\t]*(\\w+),[ \\t]*([a-zA-Z0-9#-_]+)[ \\t]*\\]!?",
      "captures": {
        "1": {
          "patterns": [
            {
              "include": "#registers"
            }
          ]
        },
        "2": {
          "patterns": [
            {
              "include": "#numerics"
            },
            {
              "include": "#registers"
            },
            {
              "include": "#variables"
            }
          ]
        }
      }
    },
